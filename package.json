  "scripts": {
    "build": "yarn run clean && babel src -d dist",
    "clean": "rimraf dist",
    "dev": "nodemon --exec babel-node src/server.js",
    "dev-populateDb": "nodemon --exec babel-node src/scripts/populateDb.js",
    "populateDb": "yarn run clean && yarn run build && cd dist && cd scripts && node populateDb.js",
    "start": "yarn run clean && yarn run build && cd dist && node server.js",
    "docker-build": "docker build . -t space-news-codesh",
    "docker-run": "docker run -p 3000:3000 -d space-news-codesh"
  },
